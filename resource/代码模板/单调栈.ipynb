   ],
   "source": [
    "nums = [3, 1, 4, 2, 5]\n",
    "stack = []\n",
    "for num in nums:\n",
    "    while stack and stack[-1] < num:\n",
    "        stack.pop()\n",
    "    stack.append(num)\n",
    "    print(stack)"
   ]
  }
